{
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "nystudio107/craft-twigpack": "1.1.3",
    "futureactivities/craft3-cloudinary": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": {
    "repo-name": {
      "type": "vcs",
      "url": "https://github.com/FutureActivities/Craft3-Cloudinary/"
    }
  }
}
